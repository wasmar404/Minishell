void	check_front_heredoc(t_token *head, t_env *envp, t_shell *shell)
{
	t_token	*temp;
	int		fd;

	temp = head->next;
	while (temp != NULL)
	{
		if (temp->type == HERE_DOC)
		{
			fd = open("temp", O_WRONLY | O_CREAT | O_TRUNC, 0644);
			if (fd < 0)
			{
				perror("open");
				return ;
			}
			heredoc(temp->next->token, fd, envp, shell);
			ft_close(fd);
		}
		temp = temp->next;
	}
	fd = open("temp", O_RDONLY);
	dup2(fd, 0);
	ft_close(fd);
	unlink("temp");
}